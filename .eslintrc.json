{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": ["eslint:recommended", "plugin:jest/recommended"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        //セミコロンを必ず付ける
        "semi": [
            "error",
            "always"
        ]
    }
}